                }, 500);               

                

            });      
                function setdisable_li(_o) {
                   
                    var _clnk = $(_o);
                    $(_o).hide();
                    var _a = $('<a>').text($(_o).text()).css({ 'cursor': 'not-allowed' });
                    $(_a).html('<i class="bi bi-people fs-4" ></i>');
                    $(_o).after($(_a));
                    setTimeout(function () { 
                        
                        $(_a).remove(); $(_clnk).show(); console.log('Show link'); }, 900);
                }

