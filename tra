<script type="text/javascript">
    function setpara(s_type, s_prod) {
        // home
        if (s_type == "gi" && s_prod == "home") {
            AjaxPageLoad(trade, 'trade', 'flowtype=insurance/inslifeinsurance&ordtype=GI&ProductName=Home&FML_POINTS=', $('#pnlMainRow'), $('#pnlMainRow'), true, 'HTML', false);
        }
        //travel
        else if (s_type == "gi" && s_prod == "travel") {
            AjaxPageLoad(trade, 'trade', 'flowtype=insurance/inslifeinsurance&ordtype=GI&ProductName=Travel&FML_POINTS=', $('#pnlMainRow'), $('#pnlMainRow'), true, 'HTML', false);
        }
        //personal
        else {
            AjaxPageLoad(trade, 'trade', 'flowtype=insurance/inslifeinsurance&ordtype=GI&ProductName=Health&FML_POINTS=', $('#pnlMainRow'), $('#pnlMainRow'), true, 'HTML', false);
        }
    }
</script>











