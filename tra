<script type="text/javascript">
    $(document).bind("keydown", function (e) {
        if (e.keyCode == 13) {
            var eventobj = e.target;
            if ($(e.target).is("a")) { return; }
            if ($(eventobj).is('input[type=button]') && $(eventobj).attr('setdefault') == undefined) { $(eventobj).click(); }
            else {
                SetEnter(e);
            }
            return false;
        }
    });
    

</script>
 
