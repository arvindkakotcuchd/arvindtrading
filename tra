<script type="text/javascript" defer >
    $(document).ready(function () {
        mktdata();
        setInterval(function () { mktdata(); }, 1800000);
        if (window.matchMedia("screen and (max-width: 1050px)").matches) {
            var _l1 = $('#pnlHeadLogin').find('.live_status');
            var _dv1 = $(_l1).find('div:first-child');
            var _dv2 = $(_l1).find('div:nth-child(2)');
            $(_dv1).removeClass('col-md-7'); $(_dv1).addClass('col-md-6'); $(_dv1).find('img').css('height', '40px');
            $(_dv2).removeClass('col-md-5'); $(_dv2).addClass('col-md-6');
        }
    });

    function mktdata() {
        PageMethodCall(dataHanlder, '', '1', 'topmarket', '', null, null, false, null, 'JSON', SetData);
    }

    function SetData(outData) {
        ///debugger;
        var obj = jQuery.parseJSON(outData.responseText);
        for (var i = 0; i < obj.length; i++) {
            if (obj[i].StockCode == "NIFTY") {
                if ($('#mktnifty').hasClass('ui-new')) {
                    $('#mktnifty').html((obj[i].HighLow == "H" ? '&uarr;' : '&darr;') + obj[i].TradePrice + ' (' + obj[i].DiffPrice + ')');
                    if (obj[i].HighLow == "H") { $('#mktnifty').addClass("text-status"); $('#mktnifty').removeClass("text-danger"); } else { $('#mktnifty').removeClass("text-status"); $('#mktnifty').addClass("text-danger"); }
                } else {
                    $('#mktnifty').text(obj[i].TradePrice + ' (' + obj[i].DiffPrice + ')');
                    if (obj[i].HighLow == "H") { $('#mktnifty').addClass("text-status"); } else { $('#mktnifty').removeClass("text-status"); }
                }
                $('#mktnifty').attr('tag-chkprevcls', "1");
                $('#mktnifty').attr('tag-prevclsval', obj[i].PrevClose);
            }
            else if (obj[i].StockCode == "SENSEX") {
                if ($('#mktsensex').hasClass('ui-new')) {
                    $('#mktsensex').html((obj[i].HighLow == "H" ? '&uarr;' : '&darr;') + obj[i].TradePrice + ' (' + obj[i].DiffPrice + ')');
                    if (obj[i].HighLow == "H") { $('#mktnifty').addClass("text-status"); $('#mktnifty').removeClass("text-danger"); } else { $('#mktnifty').removeClass("text-status"); $('#mktnifty').addClass("text-danger"); }
                } else {
                    $('#mktsensex').text(obj[i].TradePrice + ' (' + obj[i].DiffPrice + ')');
                    if (obj[i].HighLow == "H") { $('#mktsensex').addClass("text-status"); } else { $('#mktsensex').removeClass("text-status"); }
                }
                $('#mktsensex').attr('tag-chkprevcls', "1");
                $('#mktsensex').attr('tag-prevclsval', obj[i].PrevClose);
            }
            else if (obj[i].StockCode == "CNXBAN") {
                if ($('#mktbank').hasClass('ui-new')) {
                    $('#mktbank').html((obj[i].HighLow == "H" ? '&uarr;' : '&darr;') + obj[i].TradePrice + ' (' + obj[i].DiffPrice + ')');
                    if (obj[i].HighLow == "H") { $('#mktbank').addClass("text-status"); $('#mktbank').removeClass("text-danger"); } else { $('#mktbank').removeClass("text-status"); $('#mktbank').addClass("text-danger"); }
                } else {
                    $('#mktbank').text(obj[i].TradePrice + ' (' + obj[i].DiffPrice + ')');
                    if (obj[i].HighLow == "H") { $('#mktbank').addClass("text-status"); } else { $('#mktbank').removeClass("text-status"); }
                }
                $('#mktbank').attr('tag-chkprevcls', "1");
                $('#mktbank').attr('tag-prevclsval', obj[i].PrevClose);
            }
            else if (obj[i].StockCode == "NIFFIN") {
                if ($('#mktfinnif').hasClass('ui-new')) {
                    $('#mktfinnif').html((obj[i].HighLow == "H" ? '&uarr;' : '&darr;') + obj[i].TradePrice + ' (' + obj[i].DiffPrice + ')');
                    if (obj[i].HighLow == "H") { $('#mktfinnif').addClass("text-status"); $('#mktfinnif').removeClass("text-danger"); } else { $('#mktfinnif').removeClass("text-status"); $('#mktfinnif').addClass("text-danger"); }
                } else {
                    $('#mktfinnif').text(obj[i].TradePrice + ' (' + obj[i].DiffPrice + ')');
                    if (obj[i].HighLow == "H") { $('#mktfinnif').addClass("text-status"); } else { $('#mktfinnif').removeClass("text-status"); }
                }
                $('#mktfinnif').attr('tag-chkprevcls', "1");
                $('#mktfinnif').attr('tag-prevclsval', obj[i].PrevClose);
            }
            else if (obj[i].StockCode == "NIFSEL") {
                if ($('#mktnifsel').hasClass('ui-new')) {
                    $('#mktnifsel').html((obj[i].HighLow == "H" ? '&uarr;' : '&darr;') + obj[i].TradePrice + ' (' + obj[i].DiffPrice + ')');
                    if (obj[i].HighLow == "H") { $('#mktnifsel').addClass("text-status"); $('#mktnifsel').removeClass("text-danger"); } else { $('#mktnifsel').removeClass("text-status"); $('#mktnifsel').addClass("text-danger"); }
                } else {
                    $('#mktnifsel').text(obj[i].TradePrice + ' (' + obj[i].DiffPrice + ')');
                    if (obj[i].HighLow == "H") { $('#mktnifsel').addClass("text-status"); } else { $('#mktnifsel').removeClass("text-status"); }
                }
                $('#mktnifsel').attr('tag-chkprevcls', "1");
                $('#mktnifsel').attr('tag-prevclsval', obj[i].PrevClose);
            }
              else if (obj[i].StockCode == "GNIFTY") {
                  if ($('#mktgiftnif').hasClass('ui-new')) {
                      $('#mktgiftnif').html((obj[i].HighLow == "H" ? '&uarr;' : '&darr;') + obj[i].TradePrice + ' (' + obj[i].DiffPrice + ')');
                      if (obj[i].HighLow == "H") { $('#mktgiftnif').addClass("text-status"); $('#mktgiftnif').removeClass("text-danger"); } else { $('#mktgiftnif').removeClass("text-status"); $('#mktgiftnif').addClass("text-danger"); }
                  } else {
                      $('#mktgiftnif').text(obj[i].TradePrice + ' (' + obj[i].DiffPrice + ')');
                      if (obj[i].HighLow == "H") { $('#mktgiftnif').addClass("text-status"); } else { $('#mktgiftnif').removeClass("text-status"); }
                  }
                  $('#mktgiftnif').attr('tag-chkprevcls', "1");
                  $('#mktgiftnif').attr('tag-prevclsval', obj[i].PrevClose);
              }
          }
      }

    ScrollToAuthRep = function () {
        scrollwindowpos('dvAuth');
    }

</script>
 <script>
     /* quick-popup */
     $(".quick-popup").on("click", function () {
         $(this).addClass("hide")
         $(".quick-action").addClass("active");
         $(".quick-popup-content ").removeClass("hide");
     })

     $(".quick-action .close").on("click", function () {
         $(".quick-popup").removeClass("hide")
         $(".quick-action").removeClass("active");
         $(".quick-popup-content ").addClass("hide");
     })
     $(".quick-popup-content ul li").on("click", function () {
         $(".quick-popup").removeClass("hide")
         $(".quick-action").removeClass("active");
         $(".quick-popup-content ").addClass("hide");
     });
	</script>

