   <script type="text/javascript">
       function GetQuoteFno(PriceDesc) {
           window.open('https://secure.icicidirect.com/Trading/FNO/GetQuote?' + PriceDesc, 'FnOGetQuote', 'width=900,height=700,resizable=yes,scrollbars=yes');
       }

       function GetQuoteCommodity(PriceDesc) {
           window.open('https://secure.icicidirect.com/Trading/Commodity/getquote/?' + PriceDesc, 'ConnGetQuote', 'width=900,height=700,resizable=yes,scrollbars=yes');
       }

       function ClickGetQuoteCurrency(PriceDesc) {
           window.open('https://secure.icicidirect.com/Trading/currency/currencygetquote/?' + PriceDesc, 'CurrGetQuote', 'width=900,height=700,resizable=yes,scrollbars=yes')
       }

       function Clickbuysellcurrency(pgcode, otherquerystr) {
//           window.opener.buysellcurrency(pgcode, otherquerystr);
           //           window.close();           
           var trantype = otherquerystr.split('&')[0].toString();
           var stockCd = otherquerystr.split('&')[3].toString().split('=')[1].toString();
           if (trantype == "action=CurrencyFutureBuy") {
               AjaxPageLoad(trade, 'trade', 'flowtype=currency/future/' + otherquerystr, $('#pnlMainRow'), $('#pnlMainRow'), true, 'HTML', false);
           }
           else if (trantype == "action=CurrencyFutureSell") {
               AjaxPageLoad(trade, 'trade', 'flowtype=currency/future/' + otherquerystr, $('#pnlMainRow'), $('#pnlMainRow'), true, 'HTML', false);
           }
           else if (trantype == "action=CurrencyOptionBuy") {
               AjaxPageLoad(trade, 'trade', 'flowtype=currency/option/' + otherquerystr, $('#pnlMainRow'), $('#pnlMainRow'), true, 'HTML', false);
           }
           else if (trantype == "action=CurrencyOptionSell") {
               AjaxPageLoad(trade, 'trade', 'flowtype=currency/option/' + otherquerystr, $('#pnlMainRow'), $('#pnlMainRow'), true, 'HTML', false);
           }
           else if (trantype == "action=CurrencyFuturePlusBuy") {
               AjaxPageLoad(trade, 'trade', 'flowtype=currency/futureplus/' + otherquerystr, $('#pnlMainRow'), $('#pnlMainRow'), true, 'HTML', false);
           }
           else if (trantype == "action=CurrencyFuturePlusSell") {
               AjaxPageLoad(trade, 'trade', 'flowtype=currency/futureplus/' + otherquerystr, $('#pnlMainRow'), $('#pnlMainRow'), true, 'HTML', false);
           }
       }
       function ClickBuysellfromFNO(pgcode, otherquerystr) {
           //           window.opener.OpenBuysellfromFNO(pgcode, otherquerystr);

           //           window.close();
           //action=FutureBuy&FFO_ORDR_FLW=B&FFO_PRDCT_TYP=F&FFO_UNDRLYNG=LTFINA&FFO_EXER_TYP=&FFO_EXPRY_DT=&FFO_STRK_PRC=&FFO_OPT_TYP=&FFO_RQST_TYP=&FFO_CTGRY_INDSTK=&FFO_XCHNG_CD=&FFO_MIN_LOT_QTY=&FFO_TCK_SZ=&FFO_LST_TRD_PRC=&FFO_LMT_RT=&FFO_ORD_TYP=&SLTP=           
           var trantype = otherquerystr.split('&')[0].toString();
           var stockCd = otherquerystr.split('&')[3].toString().split('=')[1].toString();
           if (trantype == "action=FutureBuy") {

               AjaxPageLoad(trade, 'trade', 'flowtype=fno/future&ordtype=sell' + otherquerystr, $('#pnlMainRow'), $('#pnlMainRow'), true, 'HTML', false);
           }
           else if (trantype == "action=FuturePlusBuy") {

               AjaxPageLoad(trade, 'trade', 'flowtype=fno/futureplus/' + otherquerystr, $('#pnlMainRow'), $('#pnlMainRow'), true, 'HTML', false);
           }
           else if (trantype == "action=FutureSell") {

               AjaxPageLoad(trade, 'trade', 'flowtype=fno/future&ordtype=sell' + otherquerystr, $('#pnlMainRow'), $('#pnlMainRow'), true, 'HTML', false);
           }
           else if (trantype == "action=FuturePlusSell") {

               AjaxPageLoad(trade, 'trade', 'flowtype=fno/futureplus/' + otherquerystr, $('#pnlMainRow'), $('#pnlMainRow'), true, 'HTML', false);
           }
           else if (trantype == "action=OptionBuy") {

               AjaxPageLoad(trade, 'trade', 'flowtype=fno/option/' + otherquerystr, $('#pnlMainRow'), $('#pnlMainRow'), true, 'HTML', false);
           }
           else if (trantype == "action=OptionSell") {

               AjaxPageLoad(trade, 'trade', 'flowtype=fno/option/' + otherquerystr, $('#pnlMainRow'), $('#pnlMainRow'), true, 'HTML', false);
           }

       }

       function ClickBuysellfromCommodity(pgcode, otherquerystr) {
           
           var trantype = otherquerystr.split('&')[0].toString();
           var stockCd = otherquerystr.split('&')[3].toString().split('=')[1].toString();
           if (trantype == "action=CommFutureBuy") {

               AjaxPageLoad(trade, 'trade', 'flowtype=commodity/future/&ordtype=buy' + otherquerystr, $('#pnlMainRow'), $('#pnlMainRow'), true, 'HTML', false);
           }
           else if (trantype == "action=CommFutureSell") {

               AjaxPageLoad(trade, 'trade', 'flowtype=commodity/future/&ordtype=sell' + otherquerystr, $('#pnlMainRow'), $('#pnlMainRow'), true, 'HTML', false);
           }

           else if (trantype == "action=CommOptionBuy") {

               AjaxPageLoad(trade, 'trade', 'flowtype=commodity/option/&ordtype=buy' + otherquerystr, $('#pnlMainRow'), $('#pnlMainRow'), true, 'HTML', false);
           }
           else if (trantype == "action=CommOptionSell") {

               AjaxPageLoad(trade, 'trade', 'flowtype=commodity/option/&ordtype=sell' + otherquerystr, $('#pnlMainRow'), $('#pnlMainRow'), true, 'HTML', false);
           }
       }


       function buysellequity(pgcode, otherquerystr) {          
          // trantype=BUY&ORD_PRDCT_TYP=M&STCK_CD=GODIND&FrmC2G=Y
//           window.opener.buysellequity(pgcode, otherquerystr);
//           window.close();
           var trantyp = otherquerystr.split('&')[0].toString();
           var stockCd = "";
           if (trantyp.toString() == "action=MarginBuy") {
                stockCd = otherquerystr.split('&')[3].toString().split('=')[1].toString();
               AjaxPageLoad(trade, 'trade', 'cgain=buy&flowtype=equity/marginbuy/' + stockCd + '/NSE', $('#pnlMainRow'), $('#pnlMainRow'), true, 'HTML', false);
           }
           else if (trantyp.toString() == "action=MarginPlusBuy")
            {
                stockCd = otherquerystr.split('&')[1].toString().split('=')[1].toString();
               AjaxPageLoad(trade, 'trade', 'cgain=buy&flowtype=equity/marginplus/' + stockCd + '/NSE', $('#pnlMainRow'), $('#pnlMainRow'), true, 'HTML', false);
           }
           else if (trantyp.toString() == "action=MarginSell") {
                stockCd = otherquerystr.split('&')[3].toString().split('=')[1].toString();
               AjaxPageLoad(trade, 'trade', 'cgain=sell&flowtype=equity/marginsell/' + stockCd + '/NSE', $('#pnlMainRow'), $('#pnlMainRow'), true, 'HTML', false);
           }
           else if (trantyp.toString() == "action=MarginPlusSell") {
                stockCd = otherquerystr.split('&')[1].toString().split('=')[1].toString();
               AjaxPageLoad(trade, 'trade', 'cgain=Sell&flowtype=equity/marginplus/' + stockCd + '/NSE', $('#pnlMainRow'), $('#pnlMainRow'), true, 'HTML', false);
           }
           
       }

       function viewiclk2gain() {
         PageMethodCall(equity, 'eqclick2gain', 1, 'viewiclk2gain', '', $('#click2gain'), $('#click2gain'), true, [$('#frm')], 'HTML');
       }

       $(document).ready(function () {
           

           $(".period-tab").hide();
           $(".periodpicker").click(function () {
               $(this).parent('li').toggleClass('openme');
               $('.period-tab').toggle();
           });

           $("input[name=recommendation]").click(function () {
               var chklength = $('[name="recommendation"]:checked').length;
               var displaySelcted = $('#ddlrecommedation').text();
               if (this.checked == true) {
                   if (this.id != "ALL" && displaySelcted == "ALL" && chklength == 2) {
                       $('#ALL').prop('checked', false);
                       displaySelcted = "";
                   } else if (this.id == "ALL" && chklength > 1) {
                       $('input[type=checkbox][name=recommendation]:checked').each(function () {
                           if (this.id != "ALL")
                           $('#' +this.id).prop('checked', false);
                       });
                   }
               } else if (this.checked == false) {
                   if (chklength == 0) {
                       $('#ALL').prop('checked', true);
                   }
               }
               var SelectedValue = "";
               displaySelcted = "";
               $('input[type=checkbox][name=recommendation]:checked').each(function () {
                   SelectedValue += this.id + ",";
                   displaySelcted += this.value + " / ";
                   //alert(displaySelcted);
                   //alert(displaySelcted.includes("Margin"));
                   if (displaySelcted.includes("Margin")) {
                       //alert('entered the conditions');
                       displaySelcted = displaySelcted.replace("Margin", "Intraday");
                   }
               });
               SelectedValue = SelectedValue.substring(0, SelectedValue.length - 1);
               displaySelcted = displaySelcted.substring(0, displaySelcted.length - 3);
               $('#DDL_Recommendation').val(SelectedValue);
               $('#ddlrecommedation').text(displaySelcted);
           });
       });
   </script>

